{
  "name": "intrinio-api-client",
  "version": "0.0.1",
  "description": "Intrinio API client written in TypeScript",
  "files": [
    "build",
    "!build/**/*.map"
  ],
  "scripts": {
    "test": "jest",
    "clean": "rimraf ./build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "watch": "tsc -w",
    "lint": "tslint -p .",
    "docs": "compodoc src/"
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test",
      "pre-push": "yarn test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PointJupiter/intrinio-ts.git"
  },
  "keywords": [
    "intrinio",
    "client",
    "financial"
  ],
  "author": "Point Jupiter Developers <developers@pointjupiter.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PointJupiter/intrinio-ts/issues"
  },
  "homepage": "https://github.com/PointJupiter/intrinio-ts#readme",
  "dependencies": {
    "got": "^9.6.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.9",
    "@types/got": "^9.4.1",
    "@types/jest": "^24.0.11",
    "@types/nock": "^9.3.1",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "nock": "^10.0.6",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.3.4000"
  }
}
